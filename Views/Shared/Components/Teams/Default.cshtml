@{
}
@model IEnumerable<string>

<div class="d-grid g-2">
    <a asp-controller="Home" asp-action="Index" asp-route-teams=""  class="btn btn-outline-secondary">Home</a>
    <br />
    @foreach (string t in Model)
    {
        <a asp-controller="Home" asp-action="Index" asp-route-team="@t" 
           class="btn @(ViewBag.team == t ? "btn-primary" : "btn-outline-secondary")"
           >@t</a>
        <br />

    }
</div>