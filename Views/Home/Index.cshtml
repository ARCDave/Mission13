@{
    ViewData["Title"] = "Home Page";
}

@model List<Bowler>

<h1>Bowlers</h1>

<ul>
    @foreach (var b in Model)
    {
        <li>@b.BowlerID - @b.BowlerFirstName @b.BowlerLastName</li>
        <li>@b.BowlerAddress - @b.BowlerCity - @b.BowlerState - @b.BowlerZip</li>
        <li>@b.BowlerPhoneNumber</li>
        <li>@b.TeamID</li>
        
    }
</ul>